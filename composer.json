{
    "name": "jv/jvchat",
    "description": "jv chat , made vjchat compatible for TYPO3 LTS 12  ",
    "type": "typo3-cms-extension",
    "non-feature-branches": ["development"],
    "support": {
        "issues": "https://github.com/velletti/jvchat/issues"
    },
    "keywords": [
        "TYPO3 CMS",
        "jvchat",
        "Chat Room"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://github.com/velletti/jvchat/",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "JV\\Jvchat\\": "Classes/"
        },
        "classmap": [
            "pi1/"
        ]
    },

    "require": {
        "typo3/cms-core": "^12.4"
        "jvelletti/jv-typoscript": "^11.5"
    },
    "replace": {
        "jvchat": "self.version"
    },

    "scripts": {

    },
    "extra": {
        "typo3/cms": {
          "extension-key": "jvchat"
        }
    }
}
