<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <!--  <langChildren>1</langChildren>!-->
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>
                    LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.sheet_general
                </sheetTitle>
                <type>array</type>
                <el>
                    <display>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.conf_view
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>
                                        LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.conf_view_1
                                    </label>
                                    <value>rooms</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>My Rooms (public and Private Chats)</label>
                                    <value>myrooms</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>
                                        LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.conf_view_2
                                    </label>
                                    <value>chat</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>
                                        LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.conf_view_3
                                    </label>
                                    <value>overallusercount</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>
                                        LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.conf_view_4
                                    </label>
                                    <value>latestchat</value>
                                </numIndex>
                            </items>
                            <size>1</size>
                            <minitems>0</minitems>
                        </config>
                        <onChange>reload</onChange>
                    </display>
                    <chatroom>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.chatroom
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>tx_jvchat_room</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                        </config>
                        <displayCond>
                            <OR>
                                <numIndex index="0">FIELD:sDEF.display:=:chat</numIndex>
                                <numIndex index="1">FIELD:sDEF.display:=:latestchat</numIndex>
                            </OR>
                        </displayCond>
                    </chatroom>
                    <chatwindow>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.chat_window
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                        </config>
                        <displayCond>FIELD:sDEF.display:!=:overallusercount</displayCond>
                    </chatwindow>
                    <targetwindow>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.target_window
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:overallusercount</displayCond>
                    </targetwindow>
                </el>
            </ROOT>
        </sDEF>
        <roomsDEF>
            <ROOT>
                <sheetTitle>
                    LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.sheet_roomlist
                </sheetTitle>
                <type>array</type>
                <el>
                    <chatrooms>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.chatrooms
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>group</type>
                            <internal_type>db</internal_type>
                            <allowed>tx_jvchat_room</allowed>
                            <size>10</size>
                            <maxitems>100</maxitems>
                        </config>
                        <displayCond>FIELD:sDEF.display:!=:chat</displayCond>
                    </chatrooms>
                    <typoscriptRoomsTemplate>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.typoscriptRoomsTemplate
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>Rooms (rooms.)</label>
                                    <value>rooms</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>RoomsShort (Teaser) (roomsShort.)</label>
                                    <value>roomsShort</value>
                                </numIndex>
                            </items>
                            <size>1</size>
                            <minitems>0</minitems>
                            <default>rooms</default>
                        </config>
                    </typoscriptRoomsTemplate>
                    <showDescription>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showDescription
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </showDescription>
                    <hideEmptyRooms>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.hideEmptyRooms
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>0</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </hideEmptyRooms>
                    <hideClosedRooms>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.hideClosedRooms
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </hideClosedRooms>
                    <hidePrivateRooms>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.hidePrivateRooms
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </hidePrivateRooms>
                    <showModerators>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showModerators
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </showModerators>
                    <showUsers>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showUsers
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </showUsers>
                    <showExperts>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showExperts
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </showExperts>
                    <showUserCount>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showUserCount
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:rooms</displayCond>
                    </showUserCount>
                </el>
            </ROOT>
        </roomsDEF>
        <chatDEF>
            <ROOT>
                <sheetTitle>
                    LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.sheet_chatroom
                </sheetTitle>
                <type>array</type>
                <el>
                    <initChatWithMessagesBefore>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.initChatWithMessagesBefore
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>input</type>
                            <size>5</size>
                            <default>60</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </initChatWithMessagesBefore>
                    <reloadTimeIfRoomFull>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.reloadTimeIfRoomFull
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>input</type>
                            <size>5</size>
                            <default>10</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </reloadTimeIfRoomFull>
                    <refreshMessagesTime>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.refreshMessagesTime
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>input</type>
                            <size>5</size>
                            <default>5</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </refreshMessagesTime>
                    <refreshUserListTime>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.refreshUserListTime
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>input</type>
                            <size>5</size>
                            <default>5</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </refreshUserListTime>
                    <showFormatting>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showFormatting
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showFormatting>
                    <showEmoticons>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showEmoticons
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showEmoticons>
                    <showStyles>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showStyles
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showStyles>
                    <colorizeNicks>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.colorizeNicks
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </colorizeNicks>
                    <enableSound>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.enableSound
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </enableSound>
                    <showTime>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showTime
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showTime>
                    <showSendButton>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showSendButton
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>0</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showSendButton>
                    <showDescriptionInChat>
                        <label>
                            LLL:EXT:jvchat/Resources/Private/Language/locallang_db.xlf:tt_content.pi_flexform.showDescriptionInChat
                        </label>
                        <config>
                            <exclude>1</exclude>
                            <type>check</type>
                            <default>1</default>
                        </config>
                        <displayCond>FIELD:sDEF.display:=:chat</displayCond>
                    </showDescriptionInChat>
                </el>
            </ROOT>
        </chatDEF>
    </sheets>
</T3DataStructure>
